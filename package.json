  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "analyze": "vite build --mode analyze",
    "build:prod": "NODE_ENV=production vite build",
    "proto:generate": "protoc --ts_out frontend/src/proto --ts_opt target=ts --proto_path backend/proto backend/proto/*.proto",
    "minify-html": "node scripts/minify-html.js",
    "build:minify": "npm run build && npm run minify-html",
    "postinstall": "node -e \"const fs=require('fs');const p='node_modules/capacitor-health/Package.swift';if(fs.existsSync(p)){let c=fs.readFileSync(p,'utf8');c=c.replace('from: \\\"7.0.0\\\"','\\\"7.0.0\\\"..<\\\"9.0.0\\\"');fs.writeFileSync(p,c);}\"",
    "cap:build": "npm run build && rm -rf build/blog && npx cap copy ios",
    "cap:sync": "npm run build && rm -rf build/blog && npx cap sync ios",
    "cap:open:ios": "npx cap open ios"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
